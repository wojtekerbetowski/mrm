{
    "manifest_version": 2,
  
    "name": "MeetupRandom",
    "description": "Meetup randomizing machine",
    "version": "1.0",
        
    "permissions": [
		"http://api.meetup.com/",
		"tabs"
    ],
    "browser_action": {
		"default_icon": "meetup.png",
		"default_popup": "popup.html"
    }
}
